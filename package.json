{
  "name": "task-bar",
  "version": "1.0.0",
  "description": "A TaskBar",
  "main": "./out/main/main.js",
  "author": "zane.zyz",
  "scripts": {
    "dev:main": "tsc && export NODE_ENV=development && electron .",
    "dev:web": "webpack --config ./web/webpack.config.js --watch",
    "build": "tsc && npm run build:web && npm run build:mac && npm run build:win",
    "build:web": "webpack --config ./web/webpack.config.js",
    "build:mac": "electron-builder --dir",
    "build:win": "electron-builder build --win --publish never"
  },
  "build": {
    "appId": "com.wizards.ElectronAutoUpdate",
    "mac": {
      "category": "productivity",
      "icon": "static/app.icns"
    }
  },
  "repository": "https://github.com/zhanspace/TaskBar.git",
  "keywords": [
    "Electron",
    "termBar"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/node": "^18.6.1",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "antd": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "css-loader": "^2.1.1",
    "electron": "^8.2.3",
    "electron-positioner": "^4.1.0",
    "electron-updater": "^4.2.0",
    "electron-builder": "^22.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "indexof": "^0.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.15",
    "postcss": "^7.0.14",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^4.8.0-dev.20220726",
    "url-loader": "^0.5.7",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "fix-path": "^3.0.0",
    "menubar": "^8.0.1"
  }
}
