{
  "name": "task-bar",
  "version": "1.0.0",
  "description": "A TaskBar",
  "main": "./main/main.js",
  "author": "zane.zyz",
  "scripts": {
    "web": "webpack --config ./web/webpack.config.js",
    "web:watch": "webpack --config ./web/webpack.config.js --watch",
    "dev": "export NODE_ENV=development && electron .",
    "dev:prod": "export NODE_ENV=development && electron .",
    "build": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --win --publish always",
    "pack": "node ./tasks/pack.js"
  },
  "build": {
    "appId": "com.wizards.ElectronAutoUpdate",
    "mac": {
      "category": "productivity",
      "icon": "static/app.icns"
    }
  },
  "repository": "",
  "keywords": [
    "Electron",
    "termBar"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/react-dom": "^16.9.6",
    "antd": "^4.1.0",
    "archiver": "^3.1.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "css-loader": "^2.1.1",
    "electron": "^7.1.1",
    "electron-builder": "^22.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "indexof": "^0.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.15",
    "postcss": "^7.0.14",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.3333",
    "url-loader": "^0.5.7",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "electron-positioner": "^4.1.0",
    "electron-updater": "^4.2.0",
    "fix-path": "^3.0.0",
    "menubar": "^8.0.1"
  }
}
